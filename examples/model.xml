<?xml version="1.0" encoding="utf-8"?>
<model>
	<assetPaths basePath="assets/">
		<bitmapPath>bmp/</bitmapPath>
		<soundPath>sounds/</soundPath>
		<swfPath>swf/</swfPath>
		<flvPath>flv/</flvPath>
		<xmlPath>xml/</xmlPath>
	</assetPaths>
	
	<assets>
		<asset libraryName="jpg1" source="jpg1.jpg" />
		<asset libraryName="jpg2" source="jpg2.jpg" />
		<asset libraryName="swftest" source="swfload_test.swf" />
		<asset libraryName="pizza_song" source="Pizza_Song.flv" />
		<asset libraryName="english" source="english.xml" />
		<asset libraryName="png1" source="png1.png" />
		<asset libraryName="png2" source="png2.png" />
		<asset libraryName="sound1" source="sound1.mp3" />
	</assets>
	
	<textformats>
		<tf id="Helv" size="1" embedFonts="true" />
	</textformats>
	
	<stylesheets>
		<css id="css1" src="" />
	</stylesheets>
	
	<preload>
		<asset libraryName="jpg1" />
		<asset libraryName="jpg2" />
		<asset libraryName="swftest" />
		<asset libraryName="pizza_song" />
		<asset libraryName="english" />
		<asset libraryName="png1" />
		<asset libraryName="png2" />
		<asset libraryName="sound1" />
	</preload>
	
	<links>
		<link id="google" href="http://www.google.com/" target="_blank" />
	</link>
	
	<attributes>
		<attribute id="example" value="whateverYouWant" />
	</attributes>
	
	<services>
		<remoting>
			<endpoint id="amfphp" gateway="http://www.leftvsright.com/console/weborb.aspx" useLimiter="true" maxRetries="5" callTimeout="5000" objectEncoding="3">
				<service id="amfphpService1">com.myphp.Service1</service>
				<service id="amfphpService2">com.myphp.Service2</service>
				<service id="amfphpMRMCache">MRM_CacheManager.CacheFront</service>
			</endpoint>
			<endpoint id="rubyamf" gateway="http://localhost:8024/rubyamf/gateway.rb" useLimiter="true" maxRetries="5" callTimeout="5000" objectEncoding="3">
				<service id="rubyamfService1">com.myphp.Service1</service>
				<service id="rubyamfService2">com.myphp.Service2</service>
				<service id="rubyamfMRMCache">MRM_CacheManager.CacheFront</service>
			</endpoint>
		</remoting>
		<http>
			<service id="user" url="http://tagsf/services/codeigniter/index.php" defaultResponseFormat="variables" />
			<service id="session" url="http://tagsf/services/codeigniter/index.php" defaultResponseFormat="variables" />
			<service id="trail" url="http://tagsf/services/codeigniter/index.php" defaultResponseFormat="variables" />
		</http>
	</services>
</model>