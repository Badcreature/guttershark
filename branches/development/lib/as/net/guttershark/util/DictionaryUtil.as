package net.guttershark.util{	import flash.utils.Dictionary;			/**	 * Static class wrapping various Dictionary utilities.	 */	public class DictionaryUtil 	{		/**		 *	Return an Array of all keys in the dictionary.		 * 	@param d		 * 	@return Array		 */							public static function getKeys(d:Dictionary):Array 		{			var a:Array = new Array();			for (var key:Object in d) 			{				a.push(key);			}			return a;		}		/**		 *	Return an Array of all values in the dictionary.				 * 	@param d		 * 	@return		 */							public static function getValues(d:Dictionary):Array 		{			var a:Array = new Array();			for each (var value:Object in d) 			{				a.push(value);			}			return a;		}		}}