package net.guttershark.util.types{	import flash.utils.Dictionary;	/**	 * Static class wrapping various Dictionary utilities.	 */	public class DictionaryUtil 	{		/**		 *	Return an Array of all keys in the dictionary.		 *			 * 	@param d A dictionary who's keys will be returned.		 */							public static function keys(dict:Dictionary):Array 		{			var a:Array = [];			for(var key:Object in dict) a.push(key);			return a;		}		/**		 *	Return an Array of all values in the dictionary.		 * 			 * 	@param A dictionary who's values will be returned.		 */							public static function values(dict:Dictionary):Array		{			var a:Array = [];			for each(var value:Object in dict) a.push(value);			return a;		}	}}