package net.guttershark.util.crypt {	/**	 * Runtime Unique ID's for runtime Object management and identification.	 */	final public class RUID	{		private static var _key:String = "__RUID";		private static var _id:int = 0;			/**		 * Generate a runtime unique id.		 */		public static function create():int 		{			return _id++;		}		/**		 * Return the current RUID id.		 */		public static function getCurrentId():int 		{			return _id;			}		/**		 * Return the key property used to track RUID's.		 */		public static function getKey():String 		{			return _key;			}	}	}