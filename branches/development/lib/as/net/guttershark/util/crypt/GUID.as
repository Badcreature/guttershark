package net.guttershark.util.crypt{	import flash.system.Capabilities;		import net.guttershark.util.crypt.SHA1;		/**	 *  Genuine Unique IDentifier string.	 */	final public class GUID	{		private static var counter:int = 0;		/**		 * Creates a new Genuine Unique IDentifier.		 */		public static function create():String 		{			var id1:Number = new Date().getTime();			var id2:Number = Math.random() * Number.MAX_VALUE;			var id3:String = Capabilities.serverString;			return SHA1.calculate(id1 + id3 + id2 + counter++);		}		}}