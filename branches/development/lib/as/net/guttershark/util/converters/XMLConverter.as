package net.guttershark.util.converters{
	/**	 * The XMLConverter class provides a simple XML E4X native typing utilitiy.	 */	final public class XMLConverter 	{
				/**		 * Convert XML data to native types.		 * 		 * @param value - string value from XML		 */		public static function parseType(value:String):*		{			var v:String = value.toLowerCase();			if(!isNaN(Number(value))) return Number(value);			if(v == "true" || v == "false" || v == "0" || v == "1" || v == "yes" || v == "no" || v == "on" || v == "off") return BoolConverter.toBoolean(value);			if(v == "null") return null;			return value;		}	}}