package net.guttershark.display.draw{	/**	 * ITweenableDrawing defines the method signatures of tweenable drawn Shapes.	 */	public interface ITweenableDrawing	{		/**		 * Draw in to the graphics Shape.		 */		function draw() : void;		/**		 * Clear & draw in to the graphics Shape.		 */		function redraw() : void;		/**		 * Clear the drawn graphics Shape.		 */		function clear() : void;	}}