package net.guttershark.support.layoutmanager{	/**	 * The Arrange Class stores enumerated values you can provide	 * to the <em><code>arrange</code></em> method of the LayoutManager	 * instance.	 * 	 * @see net.guttershark.managers.LayoutManager LayoutManager Class	 */	final public class Arrange	{				/**		 * Arrange behind.		 */		public static const BEHIND:String = 'behind';				/**		 * Arrange in front.		 */		public static const INFRONT:String = 'infront';				/**		 * Bring to front.		 */		public static const BRING_TO_FRONT:String = 'bringToFront';				/**		 * Bring forward.		 */		public static const BRING_FORWARD:String = 'bringForward';				/**		 * Send backward.		 */		public static const SEND_BACKWARD:String = 'sendBackward';				/**		 * Send to back		 */		public static const SEND_TO_BACK:String = 'sendToBack';	}}