package net.guttershark.util.crypt {	import net.guttershark.util.crypt.IHash;	/**	 * Runtime Unique ID's for runtime Object management and identification.	 */	final public class RUID implements IHash 	{		private static var _key:String = "__RUID";		private static var _id:int = 0;			/**		 * Generate a runtime unique id		 * @return int		 */		public static function create():int 		{			return _id++;		}		/**		 * Return the current RUID id		 * @return int		 */		public static function getCurrentId():int 		{			return _id;			}		/**		 * Return the key property used to track RUID's		 * @return String		 */		public static function getKey():String 		{			return _key;			}	}	}